<?xml version="1.0"?>
<package format="2">
  <name>digital_io_controller</name>
  <version>0.0.0</version>
  <description>The advanced_joint_state_publisher package</description>

  <maintainer email="nicola.pedrocchi@stiima.cnr.it">nicola</maintainer>

